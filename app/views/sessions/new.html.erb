<h2>Log in</h2>

<div class="two-thirds-panel">
  <%= form_tag session_path do %>
    <div class="inputs">
      <%= label_tag :email %>
      <%= text_field_tag :email %>

      <%= label_tag :password %>
      <%= text_field_tag :password %>
    </div>
    <div class="actions">
      <%= submit_tag "Submit", class: "button__line" %>
    </div>
  <% end %>
</div>
